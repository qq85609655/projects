<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yiyun.wasteoilcustom">


    <application android:allowBackup="true" android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name" android:supportsRtl="true" android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" android:launchMode="standard">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.Login" android:launchMode="singleTask"/>

        <activity android:name=".activities.Menu" android:launchMode="singleTask"/>
        <activity android:name=".activities.Product" android:launchMode="singleTask"/>
        <activity android:name=".activities.Schedule" android:launchMode="singleTask"/>
        <activity android:name=".activities.RecReport" android:launchMode="singleTask"/>
    </application>

</manifest>